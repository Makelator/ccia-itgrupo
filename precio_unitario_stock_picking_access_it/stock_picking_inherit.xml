      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.view_picking_form"/>
      <field name="groups_id" eval="[(6,0, [ref('precio_unitario_stock_picking_access_it.precio_unitario_sp_admin_group')])]" />
